<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="monopoly.ui.scene_controllers.HomeController"
      alignment="CENTER" fx:id="root" spacing="10">
    <ImageView fx:id="img"/>
    <Button styleClass="btn" onAction="#onPlayBtnClick" text="Play"/>
    <Button styleClass="btn" onAction="#onExitBtnClick" text="Exit"/>
</VBox>
